<h1>Your expenses</h1>
<% if flash[:notice].present? %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>
<%= link_to "New expense", new_expense_path %>
<h2>List of expenses</h2>

<% @expenses.each do |expense| %>
    <p>
      <%= link_to expense.description, expense_path(expense) %>
    </p>
<% end %>

